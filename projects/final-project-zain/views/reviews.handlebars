<div class="container">
    <a href="/addreview" class="btn btn-primary"> Add review </a>
    <br>
    <br>
    {{#each data}}
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">
                {{this.name}}
            </h5>
            <p>
                {{this.comments}}
            </p>
            <p>
                Rating: {{this.rating}}
            </p>
        </div>
    </div>
    <br>
    {{/each}}
</div>